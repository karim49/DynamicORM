{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Chrome against localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:5173",
      "webRoot": "${workspaceFolder}/orm",
      "sourceMapPathOverrides": {
        "vite://src/*": "${webRoot}/src/*",
        "/src/*": "${webRoot}/src/*",
        "src/*": "${webRoot}/src/*",
        "/*": "${webRoot}/src/*"
      }
    }
  ]
}
